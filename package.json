{
  "name": "cygnus",
  "version": "1.0.0",
  "description": "Metarepo de Cygnus que agrupa los m√≥dulos backend y frontends empaquetados.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:web\"",
    "dev:backend": "npm run dev --prefix code/backend/api",
    "dev:web": "npm run dev --prefix code/frontend/web",
    "build:backend": "npm run build --prefix code/backend/api",
    "build:web": "npm run build:web --prefix code/frontend/web",
    "build:android": "npm run build --prefix code/frontend/android",
    "pack:windows": "npm run pack --prefix code/frontend/windows",
    "add:android": "npm run cap:add --prefix code/frontend/android",
    "sync:android": "npm run sync --prefix code/frontend/android",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "prettier": "^3.4.1"
  }
}
